{
    "name": "amplify-snippets",
    "displayName": "amplify-snippets",
    "description": "This extension contains familiar utility one-liners (most of the time) for VS Code.",
    "version": "0.1.0",
    "publisher": "hang-up",
    "repository": {
        "url": "https://github.com/hang-up/amp-snippets"
    },
    "engines": {
        "vscode": "^1.63.0",
        "node": ">=14.18.1"
    },
    "icon": ".github/logo.png",
    "scripts": {
        "lint": "eslint .",
        "build": "node dist/index.js"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets/snippets.json"
            }
        ]
    },
    "devDependencies": {
        "eslint": "^8.5.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-prettier": "^4.0.0",
        "prettier": "^2.5.1",
        "vsce": "^2.5.3"
    }
}
